{{#bootstrap/bs-modal id=modalId onClose=onClose as |modal|}}
  {{#modal.header}}
    <h3 class="text-xs-center">
      Past Names
    </h3>
  {{/modal.header}}

  {{#modal.body}}
    <ul>
      {{#each user.pastNames as |name|}}
        <li>{{name}}</li>
      {{else}}
        <li><i>No previous names</i></li>
      {{/each}}
    </ul>
  {{/modal.body}}
{{/bootstrap/bs-modal}}
